package templates

templ Contact() {
	<html lang="en">
		<head>
			<title>Contact — Anemone</title>
			<meta charset="UTF-8" />
			<meta name="viewport" content="width=device-width, initial-scale=1.0" />
			<link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&display=swap" rel="stylesheet" />

			<style>
				body {
					font-family: 'Inter', sans-serif;
					margin: 0;
					background: #f6f8fa;
					color: #0a2e50;
				}

				header {
					display: flex;
					justify-content: space-between;
					align-items: center;
					background-color: white;
					padding: 0.75rem 2rem;
					box-shadow: 0 2px 6px rgba(0, 0, 0, 0.1);
					position: sticky;
					top: 0;
					z-index: 1000;
				}

				header img {
					height: 40px;
					margin-right: 10px;
				}

				.brand {
					font-weight: bold;
					font-size: 1.25rem;
					color: #0a2e50;
					margin-right: 1.5rem;
				}

				.nav-links a {
					margin-right: 1rem;
					text-decoration: none;
					color: #315f8e;
					font-weight: 500;
					transition: color 0.2s;
				}

				.nav-links a:hover {
					color: #2cccc4;
				}

				.nav-links a.active {
					color: #8645a5;
					font-weight: 600;
				}

				.header-right button {
					background-color: #fa7265;
					color: white;
					border: none;
					padding: 0.5rem 1.25rem;
					border-radius: 6px;
					cursor: pointer;
					font-weight: 600;
					transition: background-color 0.2s;
				}

				.header-right button:hover {
					background-color: #4a8eac;
				}

				main {
					max-width: 600px;
					margin: 3rem auto;
					padding: 2rem;
					background: white;
					border-radius: 12px;
					box-shadow: 0 2px 8px rgba(0, 0, 0, 0.05);
					text-align: center;
				}

				h1 {
					font-size: 2rem;
					margin-bottom: 1rem;
					color: #0a2e50;
				}

				p.lead {
					font-size: 1.1rem;
					color: #315f8e;
					margin-bottom: 2rem;
				}

				form {
					display: flex;
					flex-direction: column;
					gap: 1rem;
					text-align: left;
				}

				label {
					font-weight: 500;
					color: #315f8e;
				}

				input {
					padding: 0.75rem;
					border: 1px solid #d4d4d4;
					border-radius: 6px;
					font-size: 1rem;
					font-family: inherit;
					width: 100%;
					box-sizing: border-box;
				}

				input:focus {
					border-color: #2cccc4;
					outline: none;
					box-shadow: 0 0 0 2px rgba(44, 204, 196, 0.15);
				}

				button.submit {
					background-color: #2cccc4;
					color: white;
					border: none;
					padding: 0.75rem 1.25rem;
					border-radius: 6px;
					cursor: pointer;
					font-weight: 600;
					font-size: 1rem;
					transition: background-color 0.2s;
					margin-top: 1rem;
				}

				button.submit:hover {
					background-color: #4a8eac;
				}

				.confirmation {
					display: none;
					margin-top: 2rem;
					background: #f6f8fa;
					padding: 1.5rem;
					border-radius: 10px;
					color: #0a2e50;
					box-shadow: 0 2px 6px rgba(0, 0, 0, 0.05);
				}

				.cta-button {
					display: inline-block;
					background-color: #8645a5;
					color: white;
					padding: 0.75rem 1.5rem;
					border-radius: 6px;
					font-weight: 600;
					text-decoration: none;
					margin-top: 1rem;
					transition: background-color 0.2s;
				}

				.cta-button:hover {
					background-color: #4a8eac;
				}

				footer {
					text-align: center;
					padding: 1.5rem;
					color: #4a8eac;
					font-size: 0.9rem;
					margin-top: 4rem;
				}
			</style>

			<script>
				function handleSubmit(event) {
					event.preventDefault();

					const form = document.getElementById("contact-form");
					const confirmation = document.getElementById("confirmation");

					form.style.display = "none";
					confirmation.style.display = "block";
				}
			</script>
		</head>

